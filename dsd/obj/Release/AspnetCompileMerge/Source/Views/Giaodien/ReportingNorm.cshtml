<div class="page-breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="fa fa-home"></i>
            <a href="@Locate.Url("/")">@Locate.T("Tổng quan")</a>
        </li>
        <li>
            <a>@Locate.T("Báo cáo")</a>
        </li>
        <li class="active">@Locate.T("Báo cáo dữ liệu điều tra phiếu hộ")</li>
    </ul>
</div>

<div class="page-body">
    <div class="row">
        <div class="col-xs-12 col-md-12">
            <div class="widget">
                <div class="widget-header bordered-bottom bordered-yellow">
                    <a class="widget_back_btn" href="#" title="Quay lại"><i class="fa fa-chevron-circle-left" aria-hidden="true"></i> <span>Quay lại</span></a><span class="widget-caption">@Locate.T("Báo cáo dữ lệu điều tra phiếu hộ")</span>
                    <div class="widget-buttons">
                        <a data-toggle="maximize" href="#"><i class="fa fa-expand"></i></a>
                        <a data-toggle="collapse" href="#"><i class="fa fa-minus"></i></a>
                        <a data-toggle="dispose" href="#"><i class="fa fa-times"></i></a>
                    </div>
                </div>
                <div class="widget-body no-padding">
                    <div class="dataTables_wrapper form-inline">
                        <div class="dataTables_filter RPNorm">
                            <form class="quickSearch">
                                <div class="col-md-2 col-sm-2 fix-col-2">
                                    <label>
                                        <select class="form-control">
                                            <option hidden>--Nhóm chỉ tiêu--</option>
                                            <option>chỉ tiêu 1</option>
                                            <option>chỉ tiêu 2</option>
                                        </select>
                                    </label>
                                </div>
                                <div class="col-md-2 col-sm-2 fix-col-2">
                                    <label>
                                        <select class="form-control">
                                            <option hidden>--Chỉ tiêu--</option>
                                            <option>chỉ tiêu 1</option>
                                            <option>chỉ tiêu 2</option>
                                        </select>
                                    </label>
                                </div>
                                <div class="col-md-2 col-sm-2 fix-col-2">
                                    <label>
                                        <select class="form-control">
                                            <option hidden>--Danh sách biểu--</option>
                                            <option>chỉ tiêu 1</option>
                                            <option>chỉ tiêu 2</option>
                                        </select>
                                    </label>
                                </div>
                                <div class="col-md-2 col-sm-2 fix-col-2">
                                    <label>
                                        <select class="form-control">
                                            <option hidden>--Kỳ báo cáo--</option>
                                            <option>chỉ tiêu 1</option>
                                            <option>chỉ tiêu 2</option>
                                        </select>
                                    </label>
                                </div>
                                <div class="col-md-2 col-sm-2 fix-col-2">
                                    <label>
                                        <select class="form-control">
                                            <option hidden>--Phạm vi tổng hợp--</option>
                                            <option>chỉ tiêu 1</option>
                                            <option>chỉ tiêu 2</option>
                                        </select>
                                    </label>
                                </div>
                                <div class="col-md-2 col-sm-2 fix-col-2">
                                    <label>
                                        <select class="form-control">
                                            <option hidden>--Đơn vị báo cáo--</option>
                                            <option>chỉ tiêu 1</option>
                                            <option>chỉ tiêu 2</option>
                                        </select>
                                    </label>
                                </div>
                                <div class="col-md-2 data_btn col-sm-2 fix-col-2">
                                    <button class="btn btn-info" type="submit">@Locate.T("Báo cáo")</button>
                                </div>
                            </form>
                        </div>
                        <div class="divContainer">
                            <div class="MormRPMain">
                                <div class="col-md-12">
                                    <aside class="arrow">@Locate.T("Báo cáo theo chỉ tiêu")</aside>
                                    <div class="fieldset-project">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <p><b>@Locate.T("Mã biểu"):</b> @Locate.T("1991")  </p>
                                                <p><b>@Locate.T("Tên chỉ tiêu"):</b> @Locate.T("1991")</p>
                                            </div>
                                            <div class="col-md-6">
                                                <p><b>@Locate.T("Đơn vị báo cáo"):</b> @Locate.T("1991")  </p>
                                                <p><b>@Locate.T("Đơn vị nhận báo cáo"):</b> @Locate.T("1991")</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div>
                                        <table class="table table-bordered table-hover table-striped dataTable">
                                            <thead>
                                                <tr>
                                                    <th>@Locate.T("Nội dung")</th>
                                                    <th>@Locate.T("Mã số")</th>
                                                    <th>@Locate.T("Thuộc tính 1")</th>
                                                    <th>@Locate.T("Thuộc tính 2")</th>
                                                    <th>@Locate.T("...")</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>@Locate.T("Tổng số")</td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                </tr>
                                                <tr>
                                                    <td>@Locate.T("Nội dung 1")</td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                </tr>
                                                <tr>
                                                    <td>@Locate.T("Nội dung 1.1")</td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                </tr>
                                                <tr>
                                                    <td>@Locate.T("Nội dung 1.2")</td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                    <td></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>









