@model ReportModel
<div class="page-breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="fa fa-home"></i>
            <a href="@Locate.Url("/")">@Locate.T("Tổng quan")</a>
        </li>
        <li>
            <a>@Locate.T("Báo cáo")</a>
        </li>
        <li class="active">@ViewBag.Title</li>
    </ul>
</div>

<div class="page-body">
    <div class="row">
        <div class="col-xs-12 col-md-12">
            <div class="widget">
                <div class="widget-header bordered-bottom bordered-yellow">
                    <span class="widget-caption"><i class="fa fa-list" aria-hidden="true"></i>@ViewBag.Title</span>
                    <div class="widget-buttons">
                        <a class="widget_back_btn text-btn" href="#" title="@Locate.T("Quay lại")"><i class="fa fa-chevron-circle-left" aria-hidden="true"></i> <span>@Locate.T("Quay lại")</span></a>
                        <a data-toggle="maximize" href="#"><i class="fa fa-expand"></i></a>
                        <a data-toggle="collapse" href="#"><i class="fa fa-minus"></i></a>
                        <a data-toggle="dispose" href="#"><i class="fa fa-times"></i></a>
                    </div>
                </div>
                <div class="widget-body no-padding">
                    <div class="dataTables_wrapper form-inline divContainer" id="divReport">
                      @Html.Partial("Reports",Model)
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>







