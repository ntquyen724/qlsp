<div class="page-body">
    <div class="row">
        <div class="col-xs-12 col-md-12">
            <div class="widget">
                <div class="widget-header bordered-bottom bordered-yellow">
                    <span class="widget-caption"><i class="fa fa-list" aria-hidden="true"></i>@ViewBag.Title</span>
                    <div class="widget-buttons">
                        <a data-toggle="maximize" href="#"><i class="fa fa-expand"></i></a>
                    </div>
                </div>
                <div class="widget-body no-padding widget_form_table">
                    <div class="dataTables_wrapper form-inline">
                        <div id="divNation" class="divContainer">
                            <div class="row padding-20">
                                <div class="col-md-4">
                                    <a href="@Locate.Url("/legal-text.html")">
                                        <div class="panel panel-primary">
                                            <div class="panel-heading custpanelheader">
                                                <span class="widget-caption">
                                                    @Locate.T("HỆ THỐNG VĂN BẢN VỀ CÔNG TÁC DÂN TỘC VÀ CHÍNH SÁCH DÂN TỘC")
                                                </span>
                                            </div>
                                            <div class="panel-body padding-5">
                                                <img src="/Assets/app/img/pn1.jpg" alt="@Locate.T("HỆ THỐNG VĂN BẢN VỀ CÔNG TÁC DÂN TỘC VÀ CHÍNH SÁCH DÂN TỘC")" />
                                            </div>
                                        </div>
                                    </a>
                                </div>
                                <div class="col-md-4">
                                    <a href="@Locate.Url("/form.html")">
                                        <div class="panel panel-primary ">
                                            <div class="panel-heading custpanelheader">
                                                <span class="widget-caption">
                                                    @Locate.T("CHỈ TIÊU PHÁT TRIỂN KT-XH CỦA CÁC DÂN TỘC THIỂU SỐ")
                                                </span>
                                            </div>
                                            <div class="panel-body padding-5">
                                                <img src="/Assets/app/img/pn2.jpg" alt="@Locate.T("CHỈ TIÊU PHÁT TRIỂN KT-XH CỦA CÁC DÂN TỘC THIỂU SỐ")" />
                                            </div>
                                        </div>
                                    </a>
                                </div>
                                <div class="col-md-4">
                                    <a href="@Locate.Url("/nation/list-view.html")">
                                        <div class="panel panel-primary ">
                                            <div class="panel-heading custpanelheader">
                                                <span class="widget-caption">
                                                    @Locate.T("CÁC DÂN TỘC THIỂU SỐ TỈNH LÀO CAI")
                                                </span>
                                            </div>
                                            <div class="panel-body padding-5">
                                                <img src="/Assets/app/img/pn3.jpg" alt="@Locate.T("CÁC DÂN TỘC THIỂU SỐ TỈNH LÀO CAI")" />
                                            </div>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>