@if (Session["Success"] != null)
{
    <div class="row flash flash-success">
        <ul>
            @foreach (var message in (List<string>)Session["Success"])
            {
                <li>@message</li>
            }
        </ul>
        <i class="ui-icon ui-icon-closethick close-flash"></i>
    </div>
    Session["Success"] = null;
}
